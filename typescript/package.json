{
  "name": "gatling-ts-demo",
  "version": "3.11.2",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.11.2",
    "@gatling.io/http": "3.11.2"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.11.2",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "typescript": "5.4.5"
  },
  "scripts": {
    "clean": "rimraf target results",
    "format": "prettier --write '**/*.ts'",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit && gatling-js-cli build --typescript",
    "start": "tsc --noEmit && gatling-js-cli run --typescript"
  }
}
