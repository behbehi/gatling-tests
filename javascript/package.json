{
  "name": "gatling-js-demo",
  "version": "3.14.500",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.14.500",
    "@gatling.io/http": "3.14.500"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.14.500",
    "prettier": "3.6.2",
    "rimraf": "6.0.1"
  },
  "scripts": {
    "clean": "rimraf target",
    "format": "prettier --write '**/*.js'",
    "format-check": "prettier --check '**/*.js'",
    "build": "gatling build",
    "recorder": "gatling recorder",
    "basicSimulation": "gatling run --simulation basicSimulation",
    "postApiLoad": "gatling run --simulation postApiLoad",
    "postApiStress": "gatling run --simulation postApiStress",
    "advertiseApiLoad": "gatling run --simulation advertiseApiLoad",
    "advertiseApiStress": "gatling run --simulation advertiseApiStress",
    "taxonomyApiLoad": "gatling run --simulation taxonomyApiLoad",
    "taxonomyApiStress": "gatling run --simulation taxonomyApiStress",
    "pageApiLoad": "gatling run --simulation pageApiLoad",
    "pageApiStress": "gatling run --simulation pageApiStress",
    "miniSiteApiLoad": "gatling run --simulation miniSiteApiLoad",
    "miniSiteApiStress": "gatling run --simulation miniSiteApiStress",
    "statusApiLoad": "gatling run --simulation statusApiLoad",
    "statusApiStress": "gatling run --simulation statusApiStress",
    "userPostApiLoad": "gatling run --simulation userPostApiLoad",
    "userPostApiStress": "gatling run --simulation userPostApiStress"
  }
}
