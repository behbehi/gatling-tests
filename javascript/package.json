    "userFollowersApiLoad": "gatling run --simulation userFollowersApiLoad",
    "userFollowersApiStress": "gatling run --simulation userFollowersApiStress",
    "userNotificationApiLoad": "gatling run --simulation userNotificationApiLoad",
    "userNotificationApiStress": "gatling run --simulation userNotificationApiStress",
    "userCityApiLoad": "gatling run --simulation userCityApiLoad",
    "userCityApiStress": "gatling run --simulation userCityApiStress",
    "userCountryApiLoad": "gatling run --simulation userCountryApiLoad",
    "userCountryApiStress": "gatling run --simulation userCountryApiStress"
  }
}
